@page
@model LearningSite.Web.Pages.Account.IndexModel
@{
}

<h3>Profile</h3>

<h4>Claims</h4>

<table class="table table-sm table-striped table-hover">
    <tr>
        <td>
            Name
        </td>
        <td>
            Value
        </td>
    </tr>
    @foreach (var item in Model.Claims)
    {
        <tr>
            <td>@item.Key</td>
            <td>@item.Value</td>
        </tr>
    }
</table>

<h4>Time Zones</h4>

<table class="table table-sm table-striped table-hover">
    <tr>
        <th>System Id</th>
        <th>System Name</th>
        <th>Iana Id</th>
    </tr>
    @foreach (var item in Model.TimeZones)
    {
        <tr>
            <td>@item.SystemId</td>
            <td>@item.SystemName</td>
            <td>@item.IanaId</td>
        </tr>
    }
</table>